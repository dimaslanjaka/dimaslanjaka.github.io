<% if (config.analytics && config.analytics.enable) { %>
<% if (config.analytics.GA3) { %>
<!-- Global site tag (gtag.js) - Google Analytics -->
<script>
  lazy.add('https://www.googletagmanager.com/gtag/js?id=<%- config.analytics.GA3 %>', function () {
    window.dataLayer = window.dataLayer || [];
    function gtag() { dataLayer.push(arguments); }
    gtag('js', new Date());
    console.log('google analystic loaded');
    gtag('config', '<%- config.analytics.GA3 %>');
  });
</script>
<% } %>
<% } %>