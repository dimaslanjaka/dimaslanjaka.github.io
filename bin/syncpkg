#!/usr/bin/env bash

CWD="$PWD"
DIR="$(dirname "$(realpath "$0")")"

node "$DIR/../preinstall.js"
rm -rf node_modules
npm cache clean --force
npm un static-blog-generator
# npm install -D git+https://github.com/dimaslanjaka/static-blog-generator.git#dev
npm install -D https://github.com/dimaslanjaka/static-blog-generator/raw/dev/release/development.tgz
npm version $(git describe)