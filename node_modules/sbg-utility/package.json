{
  "name": "sbg-utility",
  "version": "1.1.2",
  "description": "static-blog-generator utility",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "!**/release*/**"
  ],
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update:packer": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js",
    "update:ncu": "npx npm-check-updates --enginesNode -u -x async,progress,cheerio,jsdom",
    "update": "run-s \"update:**\"",
    "test": "yarn run clean && jest --runInBand --detectOpenHandles",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "build": "gulp build",
    "build-pack": "yarn run build && yarn run pack",
    "clean": "gulp --series clean build",
    "pack": "node packer.js --yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/static-blog-generator.git"
  },
  "keywords": [
    "static-blog-generator",
    "utility"
  ],
  "author": "Dimas Lanjaka <dimaslanjaka@gmail.com> (https://www.webmanajemen.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimaslanjaka/static-blog-generator/issues"
  },
  "homepage": "https://github.com/dimaslanjaka/static-blog-generator#readme",
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "async": "^3.2.4",
    "axios": "^1.4.0",
    "bluebird": "^3.7.2",
    "cheerio": "^1.0.0-rc.12",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/raw/bf2d8bb/release/cross-spawn.tgz",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "glob": "^10.2.6",
    "gulp": "^4.0.2",
    "hexo": "^7.0.0-rc1",
    "hexo-log": "^4.0.1",
    "hexo-util": "^3.0.1",
    "jsdom": "^21.1.2",
    "jsonc-parser": "^3.2.0",
    "micromatch": "^4.0.5",
    "minimatch": "^9.0.1",
    "node-libcurl": "^3.0.0",
    "nunjucks": "^3.2.4",
    "plugin-error": "^2.0.1",
    "progress": "^2.0.3",
    "request": "^2.88.0",
    "safelinkify": "^1.1.18",
    "slugify": "^1.6.6",
    "through2": "^4.0.2",
    "tslib": "^2.5.2",
    "upath": "^2.0.1",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@jest/environment": "^29.5.0",
    "@jest/expect": "^29.5.0",
    "@jest/globals": "^29.5.0",
    "@jest/types": "^29.5.0",
    "@tsconfig/node14": "^1.0.3",
    "@types/async": "^3.2.20",
    "@types/bluebird": "^3.5.38",
    "@types/debug": "^4.1.8",
    "@types/fs-extra": "^11.0.1",
    "@types/gulp-cache": "^0.4.6",
    "@types/jsdom": "21",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^20.2.5",
    "@types/nunjucks": "^3.2.2",
    "@types/progress": "^2.0.5",
    "@types/request": "^2.48.8",
    "@types/through2": "https://github.com/dimaslanjaka/nodejs-package-types/tarball/through2",
    "git-command-helper": "^1.2.5",
    "jest": "^29.5.0",
    "jest-config": "^29.5.0",
    "jest-mock": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "resolutions": {
    "hexo": "https://github.com/dimaslanjaka/hexo/raw/7c3a93f/releases/hexo.tgz",
    "hexo-asset-link": "https://github.com/dimaslanjaka/hexo/raw/7c3a93f/releases/hexo-asset-link.tgz",
    "hexo-cli": "https://github.com/dimaslanjaka/hexo/raw/7c3a93f/releases/hexo-cli.tgz",
    "hexo-front-matter": "https://github.com/dimaslanjaka/hexo/raw/7c3a93f/releases/hexo-front-matter.tgz",
    "hexo-log": "https://github.com/dimaslanjaka/hexo/raw/7c3a93f/releases/hexo-log.tgz",
