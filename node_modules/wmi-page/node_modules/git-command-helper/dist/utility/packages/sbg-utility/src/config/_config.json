{
    "archive_dir": "archives",
    "author": "Dimas Lanjaka",
    "browsersync": {
        "logLevel": "warn",
        "ghostMode": {
            "scroll": true
        },
        "instanceName": "uniqueString"
    },
    "categories": {
        "lowercase": false,
        "assign": null,
        "mapper": null
    },
    "category_dir": "categories",
    "code_dir": "downloads/code",
    "date_format": "YYYY-MM-DD",
    "default_category": "uncategorized",
    "default_layout": "post",
    "deploy": {
        "type": "git",
        "repo": "",
        "branch": "gh-pages",
        "username": "",
        "email": "",
        "message": "{{ now('YYYY-MM-DD HH:mm:ss') }}",
        "deployDir": "D:/Repositories/static-blog-generator/packages/sbg-utility/.deploy_git",
        "github": {
            "submodule": []
        }
    },
    "description": "Static Blog Generator Demo",
    "exclude": null,
    "external_link": {
        "enable": true,
        "field": "site",
        "safelink": {
            "enable": true,
            "exclude": [
                "my-custom-domain.com",
                "**/exclude/**",
                "**/node_modules/**",
                "**/vendor/**",
                "**/tmp/**",
                "**/dist/**"
            ],
            "redirect": [
                "https://www.webmanajemen.com/page/safelink.html?url=",
                "https://www.webmanajemen.com/page/redirector.html?url="
            ],
            "type": "base64",
            "password": "unique-password"
        },
        "exclude": [
            "webmanajemen.com",
            "web-manajemen.blogspot.com",
            "dimaslanjaka.github.io"
        ]
    },
    "filename_case": 0,
    "future": true,
    "generator": {
        "cache": true,
        "verbose": false
    },
    "highlight": {
        "enable": true,
        "line_number": true,
        "auto_detect": false,
        "tab_replace": "  ",
        "wrap": false,
        "hljs": false
    },
    "i18n_dir": ":lang",
    "ignore": null,
    "include": null,
    "index_generator": {
        "path": "",
        "per_page": 10,
        "order_by": "-date"
    },
    "keywords": null,
    "language": "en",
    "markdown": {
        "preset": "default",
        "render": {
            "html": true,
            "xhtmlOut": false,
            "langPrefix": "language-",
            "breaks": true,
            "linkify": true,
            "typographer": true,
            "quotes": "“”‘’"
        },
        "enable_rules": null,
        "disable_rules": null,
        "plugins": null,
        "anchors": {
            "level": 2,
            "collisionSuffix": "",
            "permalink": false,
            "permalinkClass": "header-anchor",
            "permalinkSide": "left",
            "permalinkSymbol": "¶",
            "case": 0,
            "separator": "-"
        }
    },
    "meta_generator": true,
    "new_post_name": ":title.md",
    "pagination_dir": "page",
    "per_page": 10,
    "permalink": ":title.html",
    "permalink_defaults": null,
    "post_asset_folder": false,
    "post_dir": "src-posts",
    "pretty_urls": {
        "trailing_index": true,
        "trailing_html": true
    },
    "prismjs": {
        "enable": false
    },
    "public_dir": "public",
    "relative_link": false,
    "render_drafts": false,
    "root": "/static-blog-generator/",
    "sitemap": {
        "path": [
            "sitemap.xml",
            "sitemap.txt"
        ],
        "rel": true,
        "tags": true,
        "categories": true
    },
    "skip_render": null,
    "source_dir": "source",
    "tag_dir": "tags",
    "tags": {
        "lowercase": false,
        "assign": null,
        "mapper": null
    },
    "theme": "butterfly",
    "time_format": "HH:mm:ss",
    "timezone": "Asia/Jakarta",
    "title": "Static Blog Generator Demo",
    "titlecase": false,
    "updated_option": "mtime",
    "url": "http://www.webmanajemen.com/static-blog-generator"
}
