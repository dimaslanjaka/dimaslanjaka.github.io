{
  "cSpell.words": [
    "Agustina",
    "Bagaimana",
    "Belajar",
    "Bluestacks",
    "Dimas",
    "genshin",
    "Kumala",
    "Lanjaka",
    "macam",
    "Maulinda",
    "memicu",
    "Menyembunyikan",
    "Minecraft",
    "Multishot",
    "Neverland",
    "Opti",
    "Optifine",
    "RPGs",
    "Sophila",
    "Unbreaking",
    "webp",
    "webtitle",
    "xampp"
  ],
  "editor.tabCompletion": "on",
  "emmet.excludeLanguages": [],
  "emmet.includeLanguages": {
    "markdown": "html"
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": true
  },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.extensionsPath": [
    "D:\\Repositories\\gh-pages\\src-posts\\The Legend Of Neverland\\.vscode"
  ],
  "platformSettings.condition": "os.release()",
  "platformSettings.platforms": {
    "default": {
      "nodes": {
        "editor.lineNumbers": "on"
      }
    },
    "win32": {
      "inherits": "default",
      "nodes": {
        "workspaceExplorer.workspaceStorageDirectory": "%userprofile%\\.vscode_workspaces",
        "editor.lineNumbers": "on"
      },
      "remove": [
        "platformSettings.placebo"
      ]
    },
    "linux": {
      "inherits": "default",
      "nodes": {
        "workspaceExplorer.workspaceStorageDirectory": "~/.vscode_workspaces",
        "editor.lineNumbers": "relative"
      }
    },
    "darwin": {
      "inherits": "linux",
      "nodes": {
        "workspaceExplorer.workspaceStorageDirectory": "~/.vscode_workspaces",
        "editor.lineNumbers": "off"
      }
    },
  },
  "files.autoSave": "afterDelay"
}