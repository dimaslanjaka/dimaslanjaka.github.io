<div id="search-form-wrap">
<% if (theme.search.insight) { %>
    <form class="search-form" itemprop="potentialAction" itemscope itemtype="https://schema.org/SearchAction">
        <meta itemprop="target" content="<%= config.url %>/p/search.html?q={search_term_string}"/>
        <input itemprop="query-input" type="text" name="search_term_string" class="ins-search-input search-form-input" placeholder="<%= __('index.search') %>" />
        <button type="submit" class="search-form-submit"></button>
    </form>
    <%- partial('search/insight') %>
<% } else if (theme.search.swiftype) { %>
    <div class="search-form">
        <input type="text" class="st-default-search-input search-form-input" placeholder="<%= __('index.search') %>" />
    </div>
    <%- partial('search/swiftype') %>
<% } else if (theme.search.baidu) { %>
    <%- partial('search/baidu') %>
<% } else { %>
    <%- search_form({text: __('index.search')}) %>
<% } %>
</div>