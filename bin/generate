#!/usr/bin/env bash
gulp copy:assets "$@"
gulp copy:posts "$@"
gulp copy:remove-inline-style "$@"
gulp generate:assets "$@"
gulp generate:template "$@"
gulp generate:posts "$@"
gulp generate:sitemap "$@"
gulp generate:label "$@"
gulp generate:index "$@"
gulp generate:after "$@"
gulp generate:minify "$@"