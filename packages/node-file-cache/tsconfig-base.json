{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "declaration": true,
    "esModuleInterop": true,
    "lib": [
      "esnext"
    ],
    "moduleResolution": "node",
    "skipDefaultLibCheck": true,
    "noImplicitThis": false,
    "resolveJsonModule": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "rootDir": "src",
    "skipLibCheck": true,
    "strict": true,
    "traceResolution": false,
    "types": [
      "node"
    ]
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules",
    "dist",
    "src/index.backup.ts"
  ],
  "include": [
    "src/index.ts"
  ]
}