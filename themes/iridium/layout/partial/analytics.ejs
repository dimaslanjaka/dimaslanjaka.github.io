<% if (config.analytics && config.analytics.enable) { %>
<% if (config.analytics.GA3) { %>
<!-- Global site tag (gtag.js) - Google Analytics -->
<script async src="https://www.googletagmanager.com/gtag/js?id=<%- config.analytics.GA3 %>"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag() { dataLayer.push(arguments); }
  gtag('js', new Date());

  gtag('config', '<%- config.analytics.GA3 %>');
</script>
<% } %>
<% } %>