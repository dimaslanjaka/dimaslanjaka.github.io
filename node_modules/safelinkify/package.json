{
  "name": "safelinkify",
  "version": "1.1.10",
  "description": "NodeJS anonymizer external links. Anonymize external links to outbound page redirector for SEO.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "readme.md",
    "./*.json",
    "postinstall.js"
  ],
  "scripts": {
    "start": "ts-node index.ts",
    "build": "tsc -p tsconfig.build.json && webpack && gulp",
    "postbuild": "node packer.js",
    "postinstall": "node postinstall.js",
    "dev": "nodemon",
    "ci": "yarn install --immutable --immutable-cache --check-cache --frozen-lockfile --check-files",
    "update": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.github/workflows/build-release.yml > .github/workflows/build-release.yml && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js && curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js && npx npm-check-updates -u -x chalk,hexo && npm install --cache tmp/npm"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dimaslanjaka/safelink.git"
  },
  "keywords": [
    "safelink",
    "outbound-links",
    "hyperlinks",
    "redirector"
  ],
  "author": {
    "email": "dimaslanjaka@gmail.com",
    "name": "Dimas Lanjaka",
    "url": "https://www.webmanajemen.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dimaslanjaka/safelink/issues"
  },
  "homepage": "https://www.webmanajemen.com/safelink",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/bluebird": "^3.5.38",
    "@types/browser-sync": "^2.26.3",
    "@types/crypto-js": "^4.1.1",
    "@types/ejs": "^3.1.1",
    "@types/gulp": "^4.0.10",
    "@types/html-minifier-terser": "^7.0.0",
    "@types/jquery": "^3.5.14",
    "@types/markdown-it-attrs": "^4.1.0",
    "@types/node": "^18.11.17",
    "@types/prettier": "^2.7.2",
    "@types/toastr": "^2.1.40",
    "@types/undertaker": "^1.2.8",
    "article-parser": "^7.2.5",
    "bluebird": "^3.7.2",
    "browser-sync": "^2.27.11",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/tarball/typescript",
    "ejs": "^3.1.8",
    "fs-extra": "^11.1.0",
    "gulp": "^4.0.2",
    "html-minifier-terser": "^7.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^8.6.6",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "nodejs-package-types": "https://github.com/dimaslanjaka/nodejs-package-types/raw/main/release/nodejs-package-types.tgz",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "resolve-typescript-plugin": "^2.0.0",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "undertaker": "^1.3.0",
    "upath": "^2.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.1.1",
    "highlight.js": "^11.7.0",
    "jquery": "^3.6.3",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "path-browserify": "^1.0.1",
    "toastr": "^2.1.4"
  }
}
