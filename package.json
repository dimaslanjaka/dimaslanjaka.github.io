{
  "name": "static-blog-generator",
  "version": "2.0.5",
  "description": "Static Blog Generator",
  "main": "./build/src/generator.js",
  "types": "./build/src/generator.d.ts",
  "files": [
    "src",
    "gulpfile.ts",
    "themes",
    "tsconfig.json",
    "README.md",
    "_config.yml",
    "source/guide",
    "source/package.json",
    "build",
    "packages",
    "bin"
  ],
  "scripts": {
    "dev": "nodemon",
    "build": "run-s build:**",
    "build:src": "fse remove build && tsc -p tsconfig.build.json",
    "build:hexo-post-parser": "cd packages/hexo-post-parser && yarn --check-files && yarn build",
    "build:safelinkify": "cd packages/safelink && yarn --check-files && yarn build",
    "test": "env TS_NODE_PROJECT='./tsconfig.mocha.json' mocha -r ts-node/register 'src/**/*.spec*.ts'",
    "php": "php -S localhost:4002 -t docs",
    "eslint": "run-s eslint:**",
    "eslint:fix": "npx eslint --fix . > dump-eslint.log",
    "depcheck": "npx depcheck > dump-unused.log",
    "page": "echo 'install custom page libraries' && run-s page:**",
    "page:install": "cd docs && npm install --production && fse remove .git .gitignore",
    "page:install2": "cd docs/page && npm install --production && fse remove .git .gitignore",
    "format": "run-p format:**",
    "format:themes-assets": "prettier --write ./themes/**/*.{ts,css,js,scss,sass,less}",
    "format:themes-layouts": "prettier --parser html -w ./themes/**/*.ejs",
    "ci": "run-s ci:**",
    "ci:self": "yarn remove hexo-post-parser safelinkify google-news-sitemap && yarn add file:./packages/hexo-post-parser file:./packages/google-news-sitemap file:./packages/hexo-post-parser/packages/persistent-cache file:./packages/safelink file:./source && yarn install --check-cache --check-files",
    "ci:submodule": "git submodule foreach yarn ci",
    "rebuild": "yarn rebuild --ignore-engines --ignore-platform",
    "reinstall": "run-s reinstall:**",
    "reinstall:npm": "npm install --ignore-engines --ignore-platform",
    "reinstall:yarn": "fse remove yarn.lock && yarn install --ignore-engines --ignore-platform"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/dimaslanjaka.github.io.git"
  },
  "keywords": [
    "static",
    "blog",
    "generator",
    "hexo",
    "static-blog-generator"
  ],
  "author": {
    "email": "dimaslanjaka@gmail.com",
    "name": "Dimas Lanjaka",
    "url": "https://webmanajemen.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dimaslanjaka/dimaslanjaka.github.io/issues"
  },
  "homepage": "https://github.com/dimaslanjaka/dimaslanjaka.github.io#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cross-port-killer": "^1.4.0",
    "cross-spawn": "^7.0.3",
    "deepmerge-ts": "^4.0.3",
    "ejs": "^3.1.7",
    "fast-xml-parser": "^4.0.7",
    "feed": "^4.2.2",
    "find-cache-dir": "^3.3.2",
    "firebase": "^9.6.11",
    "folder-hash": "^4.0.2",
    "fs-extra": "^10.0.1",
    "glob": "^7.2.0",
    "google-news-sitemap": "file:./packages/google-news-sitemap",
    "hexo-post-parser": "file:./packages/hexo-post-parser",
    "knex": "^2.0.0",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "minimatch": "^5.0.1",
    "moment-timezone": "^0.5.34",
    "node-sass": "^7.0.1",
    "persistent-cache": "file:./packages/hexo-post-parser/packages/persistent-cache",
    "safelinkify": "file:./packages/safelink",
    "sbg-source": "file:./source",
    "sqlite3": "^5.0.8",
    "static-blog-generator-themes": "file:./themes",
    "tiny-typed-emitter": "^2.1.0",
    "url-parse": "^1.5.10",
    "yargs": "^17.4.0"
  },
