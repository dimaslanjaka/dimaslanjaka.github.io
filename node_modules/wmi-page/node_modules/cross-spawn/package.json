{
  "name": "cross-spawn",
  "version": "7.0.4",
  "description": "Cross platform child_process#spawn and child_process#spawnSync",
  "keywords": [
    "spawn",
    "spawnSync",
    "windows",
    "cross-platform",
    "path-ext",
    "shebang",
    "cmd",
    "execute"
  ],
  "author": "Andr√© Cruz <andre@moxy.studio>",
  "contributors": [
    {
      "email": "dimaslanjaka@gmail.com",
      "name": "Dimas Lanjaka",
      "url": "https://www.webmanajemen.com"
    }
  ],
  "homepage": "https://github.com/moxystudio/node-cross-spawn",
  "repository": {
    "type": "git",
    "url": "git@github.com:moxystudio/node-cross-spawn.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist/lib",
    "dist/lib/util",
    "!package-lock.json",
    "!yarn.lock"
  ],
  "scripts": {
    "update:packer": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js",
    "update:ncu": "npx npm-check-updates -u -x chalk,eslint,eslint-plugin-jsdoc",
    "lint": "eslint --fix .",
    "test": "jest --env node --coverage --no-cache --runInBand",
    "pre-release": "npm t && npm run lint",
    "release": "standard-version",
    "post-release": "git push --follow-tags origin HEAD && npm publish",
    "pack": "node packer.js",
    "build": "tsc --build && npm run pack",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "shebang-command": "^2.0.0",
    "which": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@jest/globals": "^29.5.0",
    "@moxy/babel-preset": "^3.3.2",
    "@moxy/eslint-config-base": "^13.1.0",
    "@moxy/eslint-config-jest": "^13.1.0",
    "@types/node": "^20.2.5",
    "cross-spawn": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-jsdoc": "^43.0.7",
    "fs-extra": "^11.1.1",
    "git-command-helper": "^1.2.3",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "minimist": "^1.2.8",
    "mkdirp": "^3.0.1",
    "rimraf": "^5.0.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "upath": "^2.0.1"
  },
  "engines": {
    "node": ">= 8"
  },
  "overrides": {
    "deepmerge-ts": "^4"
  }
}
