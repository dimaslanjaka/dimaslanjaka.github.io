{
  "name": "sbg-utility",
  "version": "1.1.0",
  "description": "static-blog-generator utility",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "readme.md",
    "!**/*.tsbuildinfo",
    "!**/release*/**"
  ],
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update:packer": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js",
    "update:ncu": "npx npm-check-updates -u -x async,progress,cheerio,jsdom",
    "update": "run-s update:**",
    "test": "npx jest --runInBand --detectOpenHandles",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "build": "gulp build",
    "build-pack": "yarn run build && yarn run pack",
    "clean": "gulp --series clean build",
    "pack": "node packer.js --yarn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/static-blog-generator.git"
  },
  "keywords": [
    "static-blog-generator",
    "utility"
  ],
  "author": "Dimas Lanjaka <dimaslanjaka@gmail.com> (https://www.webmanajemen.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimaslanjaka/static-blog-generator/issues"
  },
  "homepage": "https://github.com/dimaslanjaka/static-blog-generator#readme",
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "async": "^3.2.4",
    "axios": "^1.4.0",
    "bluebird": "^3.7.2",
    "cheerio": "^1.0.0-rc.12",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/raw/5560156/release/cross-spawn.tgz",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "git-command-helper": "^1.2.2",
    "glob": "^10.2.3",
    "gulp": "^4.0.2",
    "hexo": "https://github.com/dimaslanjaka/hexo/raw/deff4a14bb6b7e7f4ab364e481434c84b5a3a3f5/releases/hexo.tgz",
    "hexo-log": "https://github.com/dimaslanjaka/hexo/raw/deff4a14bb6b7e7f4ab364e481434c84b5a3a3f5/releases/hexo-log.tgz",
    "hexo-util": "https://github.com/dimaslanjaka/hexo/raw/deff4a14bb6b7e7f4ab364e481434c84b5a3a3f5/releases/hexo-util.tgz",
    "jsdom": "21.1.2",
    "jsonc-parser": "^3.2.0",
    "micromatch": "^4.0.5",
    "minimatch": "^9.0.0",
    "node-libcurl": "^3.0.0",
    "nunjucks": "^3.2.4",
    "plugin-error": "^2.0.1",
    "progress": "^2.0.3",
    "request": "^2.88.0",
    "safelinkify": "^1.1.18",
    "slugify": "^1.6.6",
    "through2": "^4.0.2",
    "true-case-path": "^2.2.1",
    "tslib": "^2.5.0",
    "upath": "^2.0.1",
    "yaml": "^2.2.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/bluebird": "^3.5.38",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^11.0.1",
    "@types/gulp-cache": "^0.4.5",
    "@types/node": "^20.1.2",
    "@types/nunjucks": "^3.2.2",
    "@types/through2": "https://github.com/dimaslanjaka/nodejs-package-types/tarball/through2",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}